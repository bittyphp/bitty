{
    "name": "bittyphp/bitty",
    "description": "A tiny and simple MVC framework. No fuss, no muss, no coconuts.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Kingsley",
            "email": "bizurkur@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "container-interop/service-provider": "*",
        "bittyphp/bitty-container": "^1.0",
        "bittyphp/bitty-event-manager": "^1.0",
        "bittyphp/bitty-http": "^1.0",
        "bittyphp/bitty-middleware": "^1.0",
        "bittyphp/bitty-router": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bitty\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitty\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
